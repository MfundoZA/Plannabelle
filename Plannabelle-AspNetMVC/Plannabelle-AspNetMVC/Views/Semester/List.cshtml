@model IEnumerable<PlannabelleClassLibrary.Models.Semester>

@{
    ViewData["Title"] = "List";
}

<h1>View</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>

@foreach (var item in Model) {
    <div class="card w-80">
        <div class="card-body">
            <h5 class="card-title">Semester</h5>
            <p class="card-text">@item.StartDate</p>
            <p class="card-text">@item.EndDate</p>
            <a href="#" class="btn btn-primary">Button</a>
        </div>
    </div>
}

